    "-//OpenSymphony Group//XWork Validator 1.0.2//EN" 
    "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">
<validators>
	<field name="tenTruyCap">
		<field-validator type="requiredstring">
			<message>Tên truy cập không được bỏ trống!</message>
		</field-validator>
	</field>
	
	<field name="matKhau">
		<field-validator type="requiredstring">
			
			<message>Mật khẩu không được bỏ trống!</message>
		</field-validator>
	</field>
	<field name="hoTen">
		<field-validator type="requiredstring">
			<message>Họ tên không được bỏ trống!</message>
		</field-validator>
	</field>
	<field name="email">
		<field-validator type="requiredstring">
			<message>Email không được bỏ trống!</message>
		</field-validator>
		<field-validator type="email">
			<message>Phải nhập email hợp lệ!</message>
		</field-validator>
	</field>
</validators>