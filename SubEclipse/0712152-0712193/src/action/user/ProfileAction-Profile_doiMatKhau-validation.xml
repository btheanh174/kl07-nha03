<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE validators PUBLIC 
    "-//OpenSymphony Group//XWork Validator 1.0.2//EN" 
    "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">

<validators>
	<field name="matKhauCu">
		<field-validator type="requiredstring">
			<message key="matKhauCu.required"></message>
		</field-validator>
	</field>
	<field name="matKhauMoi">
		<field-validator type="requiredstring">
			<message key="matKhauMoi.required"></message>
		</field-validator>
		<field-validator type="expression">
			<param name="expression">matKhauMoi eq xacNhanMatKhau</param>
			<message key="required.xacNhanMatKhauSai"></message>
		</field-validator>
	</field>	
	<field name="xacNhanMatKhau">
		<field-validator type="requiredstring">
			<message key="xacNhanMatKhau.required"></message>
		</field-validator>
	</field>	
</validators>
