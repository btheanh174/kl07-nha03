<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<constant name="struts.devMode" value="true"></constant>
	<package name="store"
		extends="struts-default,tiles-default,jcaptcha4struts2-default,json-default">
		<!-- global result -->
		<global-results>
			<result name="error">/global/error.jsp</result>
		</global-results>
		<!-- end global result -->


		<!-- <action name=AdminStore_lienHeTrucTuyen"> <result name="success" type="tiles">AdminStore_Bills.tiles</result> 
			</action> -->



		<action name="AdminStore_xemHoaDon" class="action.gianhang.admin.HoaDonAction"
			method="layDanhSachHoaDon">
			<result name="success" type="tiles">AdminStore_Bills.tiles</result>
		</action>

		<action name="AdminStore_chiTietHoaDon" class="action.gianhang.admin.HoaDonAction"
			method="xemChiTietHoaDon">
			<result name="success" type="tiles">AdminStore_DetailBill.tiles
			</result>
		</action>

		<action name="AdminStore_batDauGiaoHang" class="action.gianhang.admin.HoaDonAction"
			method="xacNhanBatDauGiaoHang">
			<result name="success" type="redirect">AdminStore_xemHoaDon</result>
		</action>

		<action name="AdminStore_xemDsSanPham" class="action.gianhang.admin.SanPhamAction"
			method="layDanhSachSanPhamTheoGianHang">
			<result name="success" type="tiles">AdminStore_ViewProducts.tiles
			</result>
		</action>

		<action name="xemDanhSachSanPhamGianHang" class="action.gianhang.admin.SanPhamAction"
			method="layDanhSachSanPhamTheoGianHangTheoTrang">
			<result name="success" type="tiles">AdminStore_ViewProducts.tiles
			</result>
		</action>

		<action name="xemChiTietSanPhamGianHang" class="action.gianhang.admin.SanPhamAction"
			method="layChiTietSanPhamGianHang">
			<result name="success">/WEB-INF/tiles/all/admin/product/popup_detail.jsp
			</result>
		</action>



		<action name="xemThongSoGianHangSanPham" class="action.gianhang.admin.SanPhamAction"
			method="xemThongSoGianHangSanPham">
			<result name="success">/WEB-INF/tiles/all/admin/product/dialog_editProduct.jsp
			</result>
		</action>

		<action name="capNhatThongTinGianHangSanPham" class="action.gianhang.admin.SanPhamAction"
			method="capNhatThongTinGianHangSanPham">
			<result name="success" type="redirect">AdminStore_xemDsSanPham.action
			</result>
		</action>


		<action name="xacNhanXoaGianHangSanPham" class="action.gianhang.admin.SanPhamAction"
			method="xacNhanXoaGianHangSanPham">
			<result name="success">/WEB-INF/tiles/all/admin/product/dialog_deleteProduct.jsp
			</result>
		</action>


		<action name="xoaGianHangSanPham" class="action.gianhang.admin.SanPhamAction"
			method="xoaGianHangSanPham">
			<result name="success" type="redirect">AdminStore_xemDsSanPham.action
			</result>
		</action>


		<action name="AdminStore_Step1_themSanPham">
			<result name="success" type="tiles">AdminStore_Add_ChoiceProduct.tiles
			</result>
		</action>

		<action name="AdminStore_Step2_themSanPham" class="action.gianhang.admin.SanPhamAction"
			method="themChiTietSanPhamDuocChon">
			<result name="success" type="tiles">AdminStore_Add_ProductInformation.tiles
			</result>
			<result name="duplicate" type="tiles">AdminStore_Error.tiles</result>
		</action>


		<action name="AdminStore_Step3_themSanPham" class="action.gianhang.admin.SanPhamAction"
			method="themSanPhamVaoGianHang_Step3">
			<result name="success" type="tiles">AdminStore.tiles
			</result>
		</action>

		<action name="echo" class="action.gianhang.admin.SanPhamAction"
			method="layDanhSachSanPhamTheoDanhMuc">
			<result name="success">/WEB-INF/tiles/all/admin/product/sub_listProducts.jsp
			</result>
		</action>


		<!-- Start quản lý nhóm danh mục gian hàng -->
		<action name="AdminStore_groupCategories" class="action.gianhang.admin.DanhMucAction"
			method="layDanhSachNhomDanhMuc">
			<result name="success" type="tiles">AdminStore_GroupCategories.tiles
			</result>
		</action>

		<action name="AdminStore_themNhomDanhMuc" class="action.gianhang.admin.DanhMucAction"
			method="themNhomDanhMuc">
			<result name="success" type="redirect">AdminStore_groupCategories.action
			</result>
		</action>

		<action name="AdminStore_giaoDienThemNhomDanhMuc">
			<result name="success">/WEB-INF/tiles/all/admin/category/dialog_addGroupCategory.jsp
			</result>
		</action>

		<action name="AdminStore_giaoDienCapNhatNhomDanhMuc" class="action.gianhang.admin.DanhMucAction"
			method="layThongTinNhomDanhMuc">
			<result name="success">/WEB-INF/tiles/all/admin/category/dialog_editGroupCategory.jsp
			</result>
		</action>

		<action name="AdminStore_giaoDienXoaNhomDanhMuc" class="action.gianhang.admin.DanhMucAction"
			method="layThongTinNhomDanhMuc">
			<result name="success">/WEB-INF/tiles/all/admin/category/dialog_deleteGroupCategory.jsp
			</result>
		</action>
		
		<action name="AdminStore_capNhatNhomDanhMuc" class="action.gianhang.admin.DanhMucAction"
			method="capNhatNhomDanhMuc">
			<result name="success" type="redirect">AdminStore_groupCategories.action
			</result>
		</action>
		
		<action name="AdminStore_xoaNhomDanhMuc" class="action.gianhang.admin.DanhMucAction"
			method="xoaNhomDanhMuc">
			<result name="success" type="redirect">AdminStore_groupCategories.action
			</result>
		</action>
		

		<!-- End quản lý danh mục gian hàng -->

		<!-- Start quản lý danh mục gian hàng -->

		<action name="AdminStore_categories" class="action.gianhang.admin.DanhMucAction"
			method="layDanhSachDanhMucGianHang">
			<result name="success" type="tiles">AdminStore_Categories.tiles
			</result>
		</action>


		<action name="AdminStore_giaoDienThemDanhMucGianHang" class="action.gianhang.admin.DanhMucAction"
			method="layDanhSachNhomDanhMuc">
			<result name="success">/WEB-INF/tiles/all/admin/category/dialog_addCategory.jsp
			</result>
		</action>


		<action name="AdminStore_themDanhMucGianHang" class="action.gianhang.admin.DanhMucAction"
			method="themDanhMucGianHang">
			<result name="success" type="redirect">AdminStore_categories.action
			</result>
		</action>

		<action name="xacNhanXoaGianHangDanhMuc" class="action.gianhang.admin.DanhMucAction"
			method="xacNhanXoaGianHangSanPham">
			<result name="success">/WEB-INF/tiles/all/admin/category/dialog_deleteCategory.jsp
			</result>
		</action>

		<action name="xoaGianHangDanhMuc" class="action.gianhang.admin.DanhMucAction"
			method="xoaGianHangDanhMuc">
			<result name="success" type="redirect">AdminStore_categories.action
			</result>
		</action>


		<!-- End quản lý danh mục gian hàng -->


		<action name="AdminStore_chooseTemplates" class="action.gianhang.admin.TemplateAction"
			method="layDanhSachTemplate">
			<result name="success" type="tiles">AdminStore_ChooseTemplate.tiles
			</result>
		</action>

		<!-- end sub site -->



	</package>
</struts>